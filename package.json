{
  "version": "1.0.0",
  "private": true,
  "description": "Redux action creator utility",
  "main": "build/index.js",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "start": "npm run clean && NODE_ENV=development neutrino start --use ./neutrino/core-preset.js",
    "build": "neutrino build --use ./neutrino/core-preset.js",
    "test": "neutrino test --use ./neutrino/test.preset.js",
    "test:dev": "neutrino test --use ./neutrino/test.preset.js --watch"
  },
  "author": "Raymond Tijhaar",
  "license": "MIT",
  "devDependencies": {
    "jest": "^21.1.0",
    "neutrino": "^6.2.0",
    "neutrino-middleware-hot": "^6.2.0",
    "neutrino-preset-jest": "^6.2.0",
    "neutrino-preset-mocha": "^6.2.0",
    "neutrino-preset-node": "^6.2.0",
    "raw-loader": "^0.5.1",
    "webpack": "^3.5.6",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "ramda": "^0.24.1"
  }
}
